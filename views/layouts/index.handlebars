{{!-- delete -- same as in quotes app in class assignment --}}
<ul>
  {{#each quotes}}
  	<li>
  		<p><span class="label">ID:</span> {{this.id}}</p>
      <p><span class="label">Title:</span> {{this.title}}</p>
      <p><span class="label">Body:</span> {{this.body}}</p>

      <button data-id="{{this.id}}" class="delquote">Delete</button>

      <a href="/{{this.id}}">Post New Note</a>
  	</li>
  {{/each}}
</ul>